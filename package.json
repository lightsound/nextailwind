{
  "name": "nextailwind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check": "pnpm run --parallel --aggregate-output /^check:/",
    "check:lint": "next lint",
    "check:prettier": "prettier --check .",
    "fix": "pnpm run fix:lint && pnpm run fix:prettier",
    "fix:lint": "next lint --fix",
    "fix:prettier": "prettier --write ."
  },
  "dependencies": {
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.0.0-alpha.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "prettier": "^3.2.5",
    "tailwindcss": "4.0.0-alpha.10",
    "typescript": "^5"
  },
  "postcss": {
    "plugins": {
      "@tailwindcss/postcss": {}
    }
  }
}
