{
  "name": "nextailwind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check": "pnpm run --parallel --aggregate-output /^check:/",
    "check:lint": "next lint",
    "check:prettier": "prettier --check .",
    "fix": "pnpm run fix:lint && pnpm run fix:prettier",
    "fix:lint": "next lint --fix",
    "fix:prettier": "prettier --write ."
  },
  "dependencies": {
    "next": "15.0.0-canary.111",
    "react": "19.0.0-rc-187dd6a7-20240806",
    "react-dom": "19.0.0-rc-187dd6a7-20240806"
  },
  "devDependencies": {
    "@types/node": "^22.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.4"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
